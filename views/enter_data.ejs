    <!-- This is where Data form will go-->
    <div class = "data-form">
    <form action="">
      <div class="center">
        <label>
          Number of Matches:
          <input type="number" id="match-quantity" name="match-quantity" min="1" max="20" value="1">
        </label>
      </div>
      <div class = "match">
      <div>
        <h2>Match 1:</h2>
      </div>
      <div>
      <label>Select Role:</label>
        <label>
          Tank
          <input type="radio" name="role" id="role-select" value="T">
          
        </label>
        <label>
          Damage
          <input type="radio" name="role" id="role-select" value="D">
          
        </label>
        <label>
          Support
          <input type="radio" name="role" id="role-select" value="S">
          
        </label>
      </div>
      <div>
        <label for="time-input">Enter the time in military format:</label>
        <input type="time" id="text" name="time">
      </div>
      

      <div>
        <label>Win or Loss:</label>
        <label>
          <label>
            Win
            <input type="radio" name="win/loss" id="win/loss" value="W">
          </label>
          <label>
            Loss
            <input type="radio" name="win/loss" id="win/loss" value="L">
          </label>
        </div>
          
        </label>

  </div>
<div class="match">
  <div>
    <h2>Match 2:</h2>
  </div>
